{
  "name": "shion",
  "type": "module",
  "version": "0.13.1",
  "private": true,
  "license": "GPL-3.0-only",
  "scripts": {
    "postinstall": "simple-git-hooks",
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "badge": "istanbul-badges-readme --style=\"for-the-badge\"",
    "lint:fix": "eslint . --fix",
    "prisma:generate": "prisma generate",
    "sea-orm:generate": "cd src-tauri && cargo bin sea-orm-cli generate entity -u sqlite://../prisma/database/data.db -o src/database/models",
    "db:generate": "pnpm prisma:generate && pnpm sea-orm:generate",
    "db:migrate": "prisma migrate dev && pnpm sea-orm:generate",
    "db:reset-migrate": "rimraf prisma/database && rimraf prisma/migrations && pnpm db:migrate",
    "db:format": "prisma format",
    "tauri": "tauri",
    "tauri:dev": "tauri dev",
    "tauri:build": "node scripts/build",
    "release": "vue-tsc --noEmit && node scripts/release"
  },
